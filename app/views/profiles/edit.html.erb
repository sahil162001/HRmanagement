<section class="profile-section position-relative vh-100">
   <div class="bg-text position-absolute ">
    <div class = "container">
      <h4 class="text-white">User Profile </h4>
      <%=form_with model: Current.user.profile,url: edit_profile_path do |form|%>
          <div class="col-6 mb- text-white">
            <%= form. label :first_name %>
            <%=form.text_field :first_name, class: "form-control", placeholder: "sahil"%>
          </div>

          <div class="col-6 mb- text-white">
            <%= form. label :last_name %>
            <%=form.text_field :last_name, class: "form-control", placeholder: "rathore"%>
          </div>

          <div class="col-6 mb- text-white">
            <%= form. label :Date_of_birth %>
            <%=form.date_field :dob, class: "form-control", placeholder: "2000/01/01"%>
          </div>

          <div class="col-6 mb- text-white">
            <%= form. label :Role %>
            <%=form.text_field :role, class: "form-control", placeholder: Current.user.role,disabled: true %>
          </div>

          <div class="col-6 mb- text-white">
            <%= form. label :Skill %>
            <%=form.text_field :skill, class: "form-control", placeholder: "ruby"%>
          </div>

          <div class="col-6 mb- text-white">
            <%= form. label :gender%>
            <%=form.text_field :gender, class: "form-control", placeholder: "male/female"%>
          </div>

          <div class="col-6 mb- text-white">
            <%= form. label :phone_number%>
            <%=form.text_field :phone, class: "form-control", placeholder: "9425xxxxxx"%>
          </div>

          <div class=" col-6 mb- text-white">
            <%= form. label :Address %>
            <%=form.text_field :address, class: "form-control", placeholder: "indore"%>
          </div>

          <div class="mb- text-white">
            <%= form.submit "Update"%>				
        </div>
      <%end%>
    </div>

  </div>
</div>